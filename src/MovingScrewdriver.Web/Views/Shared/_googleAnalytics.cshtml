@if (ViewBag.BlogConfig != null && ViewBag.BlogConfig.GoogleAnalyticsShouldRender)
{
    <script>
        var _gaq = [['_setAccount', '@ViewBag.BlogConfig.GoogleAnalyticsKey'], ['_trackPageview']];
        (function (d, t) {
            var g = d.createElement(t), s = d.getElementsByTagName(t)[0];
            g.src = '//www.google-analytics.com/ga.js';
            s.parentNode.insertBefore(g, s);
        }(document, 'script'));
    </script>
}